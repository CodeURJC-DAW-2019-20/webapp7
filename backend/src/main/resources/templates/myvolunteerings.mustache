        <!-- Content -->
        <div class="page-content bg-white">
            <!-- inner page banner -->
            <div class="dez-bnr-inr overlay-black-middle" style="background-image:url(images/background/Volunteerswalk.jpg);">
                <div class="container">
                    <div class="dez-bnr-inr-entry">
                        <h1 class="text-white">Mis voluntariados</h1>
                        <!-- Breadcrumb row -->
                        <div class="breadcrumb-row">
                            <ul class="list-inline">
                                <li><a href="index.html">Inicio</a></li>
                                <li>Mis voluntariados</li>
                            </ul>
                        </div>
                        <!-- Breadcrumb row END -->
                    </div>
                </div>
            </div>
            <!-- inner page banner END -->
            <!-- contact area -->
            <div class="content-block">
                <!-- Browse Jobs -->
                <div class="section-full bg-white browse-job content-inner-2">
                    <div class="container">
                        <!--<div class="row">-->
                        <div class="class=" col-md-12 text-center "">
                            <h5 class="widget-title font-weight-700 text-uppercase">Mis voluntariados</h5>
                            <ul class="post-job-bx">
                            {{#myvolunteerings}}
                            	
                                <li>
                                    <a href="/volunteering/{{id}}">
                                        <div class="d-flex m-b30">
                                            <div class="job-post-company">
                                                <span><img src="images/logo/icon1.png"/></span>
                                            </div>
                                            <div class="job-post-info">
                                                <h4>{{name}}</h4>
                                                <ul>
                                                    <li><i class="fa fa-map-marker"></i>{{city}}</li>
                                                </ul>
                                            </div>
                                        </div>
                                        <div class="d-flex">
                                            <div class="job-time mr-auto">
                                                <span>{{category}}</span>
                                            </div>
                                            <div class="pagination-bx">
                                                <span>{{ong}}</span>
                                            </div>
                                        </div>
                                        <span class="post-like fa fa-heart-o"></span>
                                    </a>
                                </li>
                              {{/myvolunteerings}}  
                            </ul>
                         
                        </div>

                        <!--</div>-->
                    </div>
                </div>
                <!-- Browse Jobs END -->
            </div>
        </div>
        <!-- Content END-->
